                  <option value="9000000">₹90,00,000</option>
                  <option value="9500000">₹95,00,000</option>
                  <option value="10000000">₹1,00,00,000</option>
                </select>
                <div class="dropDownSelect2"></div>
              </div>
            </div>
            <input type="text" name="mobile" style="display: none" />
            <div class="wrap-input100 bg0 rs1-alert-validate">
              <span class="label-input100">Message</span>
              <textarea
                class="input100"
                name="message"
                placeholder="Your message here..."
              ></textarea>
            </div>
            <div class="container-contact100-form-btn">
              <button class="contact100-form-btn" type="submit">
                <span>
                  Submit
                  <i
                    class="fa fa-long-arrow-right m-l-7"
                    aria-hidden="true"
                  ></i>
                </span>
              </button>
            </div>
          </form>

          <!-- Loader popup -->
          <div id="loader" style="display: none">Loading...</div>

          <!-- Success/Error message -->
          <div id="message" style="display: none"></div>
        </div>
      </div>
    </div>
  </div>
</section>
